{
  "schemaVersion": 1,
  "id": "dramaticdoors",
  "version": "${version}",

  "name": "Dramatic Doors (Fabric)",
  "description": "Fabric version of Dramatic Doors, the mod that adds three block tall doors! End Endermen discrimination!",
  "authors": [
    "Fizzware (Original creator)", "Kitteh6660 (Fabric port)"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/dramatic-doors",
    "sources": "https://github.com/TheFizzWare/DramaticDoors"
  },

  "license": "MIT",
  "icon": "assets/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.fizzware.dramaticdoors.DramaticDoors"
    ],
    "client": [
      "com.fizzware.dramaticdoors.client.RenderHandler"
    ]
  },
  "mixins": [
    "dramaticdoors.mixins.json"
  ],
  "accessWidener": "dramaticdoors.accesswidener",

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "doubledoors": "*",
    "bewitchment": "*",
    "ceilands": "*",
    "ecologics": "*",
    "twilightforest": "*"
  }
}
